
public class BlackJackCard extends Card{
	public BlackJackCard() {
		super();
	}
	
	public int getValue() {
		// enables you to build the value for the game into the card
		// this makes writing the whole program a little easier
		if (getFace() == 1)
			return 11;
		else if (getFace() >= 10)
			return 10;
		return 0;
	}
}
