	}

	public Set<String> FindDupes(String[] words) {
		// TODO Auto-generated constructor stub
		final Set<String> setToReturn = new HashSet<>(); 
		final Set<String> set1 = new HashSet<>();
		for (String yourInt : words)
		  {
		   if (!set1.add(yourInt))
		   {
		    setToReturn.add(yourInt);
		   }
		  }
		return setToReturn;
	}
	

}
